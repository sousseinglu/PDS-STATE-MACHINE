<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>MessageSubscriber</servlet-name>
    <servlet-class>metier.ViewMessageSubscriber</servlet-class>
    <load-on-startup>1</load-on-startup>
</servlet>
  <servlet>
  	<servlet-name>StateMachineCreation</servlet-name>
  	<display-name>StateMachineCreation</display-name>
  	<description></description>
  	<servlet-class>metier.ControllerStateMachineCreation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TestProducerPresentation</servlet-name>
  	<display-name>TestProducerPresentation</display-name>
  	<description></description>
  	<servlet-class>metier.TestProducerPresentation</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SsoManager</servlet-name>
  	<display-name>SsoManager</display-name>
  	<description></description>
  	<servlet-class>metier.sso.SsoManager</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StateMachineCreation</servlet-name>
  	<url-pattern>/ControllerStateMachineCreation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TestProducerPresentation</servlet-name>
  	<url-pattern>/TestProducerPresentation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SsoManager</servlet-name>
  	<url-pattern>/SsoManager</url-pattern>
  </servlet-mapping>


</web-app>
